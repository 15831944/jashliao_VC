Sub Test()

	Dim obj

	Set obj = CreateObject("Excel.Application")

	If Not obj Is Nothing Then

		obj.Visible = True
		MsgBox "Excel is running"
		obj.Quit
		Set obj = Nothing

	End If

	Set obj = CreateObject("Word.Application")

	If Not obj Is Nothing Then

		obj.Visible = True
		MsgBox "Word is running"
		obj.Quit
		Set obj = Nothing

	End If

End Sub

Test