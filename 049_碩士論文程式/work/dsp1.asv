clear;
close all;
n=0:1:49;
M=50
h=(0.42-0.5*cos(2*pi.*n/(M-1))+0.08*cos(2*pi.*n/(M-1)))/20.58
figure(1)
plot(h)
title('h[n]')

n1=0:1:399;
f=5
x1=sin(2*pi*f*n1/1000)
figure(2)
plot(x1)
title('x1[n]')

y1=conv(h,x1)
figure(3)
plot(y1)
title('y1[n]')

f=15
x2=sin(2*pi*f*n1/1000)
figure(4)
plot(x2)
title('x2[n]')

y2=conv(h,x2)
figure(5)
plot(y2)
title('y2[n]')

f=50
x3=sin(2*pi*f*n1/1000)
figure(6)
plot(x3)
title('x3[n]')

y3=conv(h,x3)
figure(7)
plot(y3)
title('y3[n]')